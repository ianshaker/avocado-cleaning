    border-radius: 20px;
    border: 1px solid rgba(255, 255, 255, 0.2);
    box-shadow: 0 8px 32px rgba(0, 0, 0, 0.1);
}

.custom-footer-left {
    flex: 1;
}

.custom-package-title-footer {
    font-size: 28px;
    font-weight: 700;
    color: white;
    margin: 0;
    text-shadow: 0 2px 4px rgba(0, 0, 0, 0.3);
}

.custom-package-subtitle-footer {
    font-size: 15px;
    color: #7f8c8d;
    margin: 0;
    font-weight: 500;
}

.custom-footer-right {
    display: flex;
    align-items: center;
    gap: 20px;
}

.custom-phone-input-new {
    flex: 1;
    padding: 12px 20px;
    border: none;
    border-radius: 18px;
    font-size: 1rem;
    font-family: 'Montserrat', sans-serif;
    background: linear-gradient(145deg, #ffffff, #f8f9fa);
    color: #2d5a3d;
    min-width: 0;
    position: relative;
    transform: translateY(0px);
    transition: all 0.3s cubic-bezier(0.4, 0, 0.2, 1);
    font-weight: 500;
    text-align: center;
    width: 200px;
    /* Многослойный объёмный эффект с зелёными оттенками */
    box-shadow: 
        /* Основная тень кнопки (зелёная) */
        0 6px 0 #4a7c59,
        /* Глубокая тень для объёма */
        0 8px 15px rgba(74, 124, 89, 0.3),
        /* Внутренняя подсветка */
        inset 0 1px 0 rgba(255, 255, 255, 0.9),
        /* Внешнее свечение */
        0 0 20px rgba(74, 124, 89, 0.2);
}

.custom-phone-input-new:focus {
    outline: none;
    background: linear-gradient(145deg, #ffffff, #f8f9fa);
    transform: translateY(-2px);
    /* Увеличенный объёмный эффект при фокусе */
    box-shadow: 
        /* Основная тень кнопки (увеличена) */
        0 8px 0 #4a7c59,
        /* Глубокая тень для объёма */
        0 12px 25px rgba(74, 124, 89, 0.4),
        /* Внутренняя подсветка */
        inset 0 1px 0 rgba(255, 255, 255, 1),
        /* Внешнее свечение (усилено) */
        0 0 30px rgba(74, 124, 89, 0.3);
}

.custom-phone-input-new::placeholder {
    color: rgba(45, 90, 61, 0.6);
}

.custom-package-button-new {
    background: linear-gradient(145deg, #ffffff, #f8f9fa);
    color: #2d5a3d;
    border: none !important;
    outline: none !important;
    outline-width: 0 !important;
    outline-style: none !important;
    outline-color: transparent !important;
    border-width: 0 !important;
    border-style: none !important;
    border-color: transparent !important;
    padding: 12px 20px;
    font-size: 1.05rem;
    font-weight: 700;
    border-radius: 18px;
    cursor: pointer;
    transition: all 0.3s cubic-bezier(0.4, 0, 0.2, 1);
    white-space: nowrap;
    position: relative;
    transform: translateY(0px);
    /* Многослойный объёмный эффект с зелёными оттенками */
    box-shadow: 
        /* Основная тень кнопки (зелёная) */
        0 6px 0 #4a7c59,
        /* Глубокая тень для объёма */
        0 8px 15px rgba(74, 124, 89, 0.3),
        /* Внутренняя подсветка */
        inset 0 1px 0 rgba(255, 255, 255, 0.9),
        /* Внешнее свечение */
        0 0 20px rgba(74, 124, 89, 0.2);
}

.custom-package-button-new:hover {
    background: linear-gradient(145deg, #ffffff, #f8f9fa);
    transform: translateY(-2px);
    border: none !important;
    outline: none !important;
    outline-width: 0 !important;
    outline-style: none !important;
    outline-color: transparent !important;
    border-width: 0 !important;
    border-style: none !important;
    border-color: transparent !important;
    /* Увеличенный объёмный эффект при наведении */
    box-shadow: 
        /* Основная тень кнопки (увеличена) */
        0 8px 0 #4a7c59,
        /* Глубокая тень для объёма */
        0 12px 25px rgba(74, 124, 89, 0.4),
        /* Внутренняя подсветка */
        inset 0 1px 0 rgba(255, 255, 255, 1),
        /* Внешнее свечение (усилено) */
        0 0 30px rgba(74, 124, 89, 0.3);
}

.custom-package-button-new:active {
    transform: translateY(2px);
    border: none !important;
    outline: none !important;
    outline-width: 0 !important;
    outline-style: none !important;
    outline-color: transparent !important;
    border-width: 0 !important;
    border-style: none !important;
    border-color: transparent !important;
    /* Эффект нажатия - уменьшение объёма */
    box-shadow: 
        /* Уменьшенная тень при нажатии */
        0 2px 0 #4a7c59,
        /* Минимальная глубокая тень */
        0 4px 8px rgba(74, 124, 89, 0.3),
        /* Внутренняя подсветка */
        inset 0 1px 0 rgba(255, 255, 255, 0.2),
        /* Уменьшенное свечение */
        0 0 10px rgba(74, 124, 89, 0.2);
}

/* УБИРАЕМ ВСЕ ВОЗМОЖНЫЕ ОБВОДКИ НАВСЕГДА */
.custom-package-button-new,
.custom-package-button-new:focus,
.custom-package-button-new:focus-visible,
.custom-package-button-new:focus-within {
    border: none !important;
    outline: none !important;
    outline-width: 0 !important;
    outline-style: none !important;
    outline-color: transparent !important;
    outline-offset: 0 !important;
    border-width: 0 !important;
    border-style: none !important;
    border-color: transparent !important;
}



/* Clean WhatsApp Icon - Remove all backgrounds and borders */
.package-whatsapp-icon-new {
    width: 60px;
    height: 60px;
    display: flex;
    align-items: center;
    justify-content: center;
    background: none;
    border: none;
    border-radius: 0;
    transition: all 0.3s ease;
    flex-shrink: 0;
    padding: 0;
}

.package-whatsapp-icon-new:hover {
    transform: scale(1.1);
    filter: drop-shadow(0 4px 12px rgba(37, 211, 102, 0.3));
}

.package-whatsapp-icon-new img {
    width: 60px;
    height: 60px;
    border-radius: 30px;
    transition: all 0.3s ease;
}

.package-whatsapp-icon-new:hover img {
    transform: rotate(5deg);
}

/* ========================================
   RESPONSIVE DESIGN
   ======================================== */

/* Responsive Design */
@media (max-width: 1024px) {
    .custom-package-content {
        grid-template-columns: 1fr 1fr;
        gap: 25px;
    }
    
    .custom-section:last-child {
        grid-column: 1 / -1;
    }
}

@media (max-width: 768px) {
    .custom-package {
        padding: 20px;
        margin-top: 30px;
    }
    
    .custom-package-content {
        grid-template-columns: 1fr;
        gap: 20px;
    }
    
    .custom-package-title {
        font-size: 24px;
    }
    
    .custom-section {
        padding: 20px;
    }
    
    .room-counter {
        padding: 10px 12px;
    }
    
    .counter-btn {
        width: 28px;
        height: 28px;
        font-size: 16px;
    }
    
    .custom-package-button {
        min-width: 200px;
        font-size: 16px;
        padding: 12px 25px;
    }

    /* Responsive adjustments for blur section */
    .custom-unified-content {
        flex-direction: column;
        gap: 20px;
        padding: 20px;
        border-radius: 16px;
    }
    
    .custom-package-footer-new {
        flex-direction: column;
        gap: 20px;
        text-align: center;
        padding: 20px;
        border-radius: 16px;
    }
    
    .custom-footer-right {
        flex-direction: column;
        gap: 15px;
        width: 100%;
    }
    
    .custom-phone-input-new {
        width: 100%;
        border-radius: 16px;
    }
    
    .custom-package-button-new {
        width: 100%;
        border-radius: 16px;
    }
    
    .cleaning-type-buttons-compact {
        flex-wrap: wrap;
        justify-content: flex-start;
        gap: 8px;
    }

    .cleaning-type-btn-compact {
        flex: 0 0 auto;
        min-width: fit-content;
    }
    
    .service-toggles-ultra-compact {
        grid-template-columns: 1fr;
    }
    
    .property-type-buttons {
        grid-template-columns: repeat(auto-fit, minmax(80px, 1fr));
    }
}